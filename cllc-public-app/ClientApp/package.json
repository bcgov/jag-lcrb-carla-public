{
  "name": "lclb_public_app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "preinstall": "npx npm-force-resolutions && npx browserslist --update-db",
    "start": "set NODE_ENV=development && ng serve --host 0.0.0.0",
    "build": "ng build --progress=false --verbose=false --statsJson=false --base-href /lcrb/",
    "builduat": "ng build --progress=false --verbose=false --statsJson=false --base-href /uat/lcrb/",
    "buildprod": "ng build --progress=false --verbose=false --statsJson=false --base-href /",
    "watch": "ng build --watch --base-href /lcrb/",
    "build:ssr": "npm run build -- --app=ssr --output-hashing=media",
    "test": "ng test --watch=false",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "eslint src/ --ext .jsx,.js,.ts,.tsx",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --loglevel=warn --check \"./src/**/*.{js,jsx,ts,tsx,html,css,scss}\"",
    "format-fix": "prettier --loglevel=warn --write \"./src/**/*.{js,jsx,ts,tsx,json,html,css,scss}\"",
    "e2e": "ng e2e"
  },
  "engine": {
    "node": ">=16.10.0",
    "npm": ">=7.24.0"
  },
  "private": true,
  "dependencies": {
